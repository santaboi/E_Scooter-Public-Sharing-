doctype html
head
  //url : https://luffy.ee.ncku.edu.tw/~santaboi/NCKU-E_scooter-app/dist/parcel_app/index.html
  meta(charset='utf-8')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  title E_Scooter 
  link(href='https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css', rel='stylesheet', integrity='sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3', crossorigin='anonymous')
  link(rel="stylesheet",type="text/css",href="./index.sass")
  script(src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js")
  script(src="index.js")
  //api key 拔掉了
  script(async defer src=src="https://maps.googleapis.com/maps/api/js?key=&callback=init_GMap&v=weekly")
body 
  div#preloader
    h1#NCKU_header
      | NCKU
      br
      | E SCOOTER
  div.container#main_block
    div.row#navbar_row
      //button.btn.btn-primary(type='button', data-bs-toggle='offcanvas', data-bs-target='#offcanvasWithBothOptions', aria-controls='offcanvasWithBothOptions') Enable
      //nav.navbar.navbar-light.bg-light#nav_bar
        div.container-fluid
          button.navbar-toggler(type='button', data-bs-toggle='offcanvas', data-bs-target='#offcanvasNavbar', aria-controls='offcanvasNavbar')
            span.navbar-toggler-icon
          //#offcanvasNavbar.offcanvas.offcanvas-end(tabindex='-1', aria-labelledby='offcanvasNavbarLabel')
            .offcanvas-header
              h5#offcanvasNavbarLabel.offcanvas-title Offcanvas
              button.btn-close.text-reset(type='button', data-bs-dismiss='offcanvas', aria-label='Close')
            .offcanvas-body
              ul.navbar-nav.justify-content-end.flex-grow-1.pe-3
                li.nav-item
                  a.nav-link.active(aria-current='page', href='#') Home
                li.nav-item
                  a.nav-link(href='#') Link
                li.nav-item.dropdown
                  a#offcanvasNavbarDropdown.nav-link.dropdown-toggle(href='#', role='button', data-bs-toggle='dropdown', aria-expanded='false')
                    | Dropdown
                  ul.dropdown-menu(aria-labelledby='offcanvasNavbarDropdown')
                    li
                      a.dropdown-item(href='#') Action
                    li
                      a.dropdown-item(href='#') Another action
                    li
                      hr.dropdown-divider
                    li
                      a.dropdown-item(href='#') Something else here
              form.d-flex
                input.form-control.me-2(type='search', placeholder='Search', aria-label='Search')
                button.btn.btn-outline-success(type='submit') Search
    div.row#main_row 
      div.column#map
